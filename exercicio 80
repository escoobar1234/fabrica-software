pessoas = 5
idade = []
altura = []

for i in range(pessoas):
    idade.append(int(input(f"digite a idade da pessoa{i+1}")))
    altura.append(float(input(f"digite a altura da pessoa {i+1}")))
for i in range(-1,-6,-1):
    print("-"*5)
    print(f"{idade[i]}")
    print(f"{altura[i]:.2f}")
    print("-" * 5)
